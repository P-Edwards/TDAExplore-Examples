#/usr/bin/bash

MYDIR="$(dirname "$(readlink -f "$0")")"
RUNFILE="$MYDIR/../../../TDAExplore-ML/ml-tda"
RETURNDIR="$PWD"

cd $MYDIR
"$RUNFILE" --parameters binucleate.csv --svm FALSE --cores $1 --plot FALSE --pca FALSE --multisvm TRUE --imagedir computation_results --ratio 2.0 --radius 75
cd $RETURNDIR
