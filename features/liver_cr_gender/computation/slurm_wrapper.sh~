#!/bin/bash
#SBATCH --job-name=classifier_training   #Job name	
#not SBATCH --partition=hpg1-compute 
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=32 # 32
#SBATCH --mem=40gb   # Per processor memory 108
#SBATCH --time=1:00:00   # Walltime
#SBATCH --qos=bubenik-b
#SBATCH --output=output_logs/classifier_training.%j.out   # Name output file 
#SBATCH --mail-type=END
#SBATCH --mail-user=pedwards@ufl.edu
#Record the time and compute node the job ran on
date; hostname; pwd
#Use modules to load the environment for R
module load R



time ./compute.sh $SLURM_CPUS_ON_NODE